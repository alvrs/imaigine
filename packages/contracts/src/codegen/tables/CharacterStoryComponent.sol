    _counters[5] = uint40(bytes(activity).length);
    _counters[6] = uint40(bytes(alignment).length);
    PackedCounter _encodedLengths = PackedCounterLib.pack(_counters);

    return
      abi.encodePacked(
        _encodedLengths.unwrap(),
        bytes((name)),
        bytes((pet)),
        bytes((age)),
        bytes((food)),
        bytes((universe)),
        bytes((activity)),
        bytes((alignment))
