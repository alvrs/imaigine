      _start = _end;
      _end += _encodedLengths.atIndex(4);
      _table.universe = (string(SliceLib.getSubslice(_blob, _start, _end).toBytes()));

      _start = _end;
      _end += _encodedLengths.atIndex(5);
      _table.activity = (string(SliceLib.getSubslice(_blob, _start, _end).toBytes()));

